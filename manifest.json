{
  "manifest_version": 2,

  "name": "Flatiron Antisocial",
  "description": "Learn.co Quiet Mode",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "content_scripts": [
    {
      "matches": ["https://learn.co/*"],
      "js": ["jquery-2.1.4.min.js"],
      "run_at": "document_end"
    }
  ]
}